<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="VectorButton">
        <attr name="drawableLeftCompat" format="reference" />
        <attr name="animateDrawableLeftCompat" format="reference" />

        <attr name="drawableTopCompat" format="reference" />
        <attr name="animateDrawableTopCompat" format="reference" />

        <attr name="drawableRightCompat" format="reference" />
        <attr name="animateDrawableRightCompat" format="reference" />

        <attr name="drawableBottomCompat" format="reference" />
        <attr name="animateDrawableBottomCompat" format="reference" />
    </declare-styleable>

    <declare-styleable name="SwitchLayout">
        <attr name="init" format="enum">
            <enum name="content" value="0"/>
            <enum name="error" value="1"/>
            <enum name="loading" value="2"/>
        </attr>
    </declare-styleable>

</resources>